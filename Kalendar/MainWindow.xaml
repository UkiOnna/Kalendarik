<Window x:Class="Kalendar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kalendar"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="700">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="200*"/>
            
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="51*"/>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="200*"/>
            <!--<RowDefinition Height="200*"/>-->
        </Grid.RowDefinitions>
        
        <Button Content="Sun" Grid.Column="0"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1" />
        <Button Content="Mon" Grid.Column="1"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1"/>
        <Button Content="Tue" Grid.Column="2"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1"/>
        <Button Content="Wed" Grid.Column="3"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1"/>
        <Button Content="Thu" Grid.Column="4"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1"/>
        <Button Content="Fri" Grid.Column="5"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1"/>
        <Button Content="Sat" Grid.Column="6"   Grid.Row="0" Background="#FF040404" Foreground="#FFF9F1F1"/>

        <Button Content="1" Grid.Column="0"   Grid.Row="1" Background="#FF191919" Foreground="#FFF9F9F9"/>
        <Button Content="2" Grid.Column="1"   Grid.Row="1" Background="#FF191919" Foreground="#FFF9F9F9"/>
